{
    "name": "jefte/larams",
    "description": "CMS",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jefte\\Larams\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jecar",
            "email": "carojefte@gmail.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "illuminate/support": "^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jefte\\Larams\\LaramsServiceProvider"
            ],
            "aliases": {
                "Larams": "Jefte\\Larams\\Facades\\Larams"
            }
        }
    }
}
